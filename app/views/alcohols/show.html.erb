<div class="page-header">
  <%= link_to alcohols_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_alcohol_path(@alcohol), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show alcohol</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @alcohol.name %></dd>

  <dt>Slug:</dt>
  <dd><%= @alcohol.slug %></dd>
</dl>
